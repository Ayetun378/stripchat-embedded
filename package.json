{
  "name": "@stripchat/stripchat-embedded",
  "version": "1.0.0",
  "description": "Stripchat JS scripts for embedding on other sites",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "buildDev": "NODE_ENV=development rollup src/index.js --config --sourcemap --output dist/index.js",
    "build": "NODE_ENV=production rollup src/index.js --config --sourcemap --output dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "devDependencies": {
    "rimraf": "2.6.2",
    "rollup": "0.56.1",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-uglify": "3.0.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stripchat-team/stripchat-embedded.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/stripchat-team/stripchat-embedded/issues"
  },
  "homepage": "https://github.com/stripchat-team/stripchat-embedded#readme"
}
